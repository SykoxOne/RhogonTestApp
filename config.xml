<?xml version='1.0' encoding='utf-8'?>
<widget id="dk.webtown.Jannick" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Weather</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <feature name="http://api.phonegap.com/1.0/geolocation"/>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-crosswalk-webview-pgb-adapt" source="npm" version="1.4.0-dev-5"/>
    <plugin name="org.apache.cordova.geolocation" />
    <plugin name="org.apache.cordova.statusbar" />
    <allow-navigation href="http://*/*" />
    <allow-navigation href="https://*/*" />
    <allow-navigation href="data:*" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="Fullscreen" value="false"/>
    </platform>
    <preference name="phonegap-version" value="cli-6.1.0" />
    <preference name="xwalkCommandLine" value="--disable-pull-to-refresh-effect" />
    <preference name="xwalkVersion" value="org.xwalk:xwalk_core_library:18+" />
    <preference name="xwalkVersion" value="xwalk_core_library:18+" />
    <preference name="xwalkVersion" value="18+" />
    <preference name="xwalkVersion" value="18" />
    <preference name="android-windowSoftInputMode" value="adjustPan" />
    <preference name="StatusBarOverlaysWebView" value="true" />
</widget>
